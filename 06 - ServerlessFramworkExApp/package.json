{
  "name": "serverless-enterprise-app",
  "version": "1.0.0",
  "description": "Enterprise-level serverless application with comprehensive business architecture",
  "main": "src/index.js",
  "scripts": {
    "dev": "sls offline start",
    "deploy:dev": "sls deploy --stage dev",
    "deploy:staging": "sls deploy --stage staging",
    "deploy:prod": "sls deploy --stage prod",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "jest --config jest.e2e.config.js",
    "lint": "eslint src/**/*.{js,ts}",
    "lint:fix": "eslint src/**/*.{js,ts} --fix",
    "type-check": "tsc --noEmit",
    "build": "tsc",
    "logs:dev": "sls logs -f api --stage dev -t",
    "logs:prod": "sls logs -f api --stage prod -t",
    "invoke:local": "sls invoke local -f api",
    "remove:dev": "sls remove --stage dev",
    "remove:staging": "sls remove --stage staging"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.540.0",
    "@aws-sdk/client-s3": "^3.540.0",
    "@aws-sdk/client-secrets-manager": "^3.540.0",
    "@aws-sdk/client-sns": "^3.540.0",
    "@aws-sdk/client-sqs": "^3.540.0",
    "@aws-sdk/client-ssm": "^3.540.0",
    "@aws-sdk/lib-dynamodb": "^3.540.0",
    "@middy/core": "^5.5.1",
    "@middy/http-cors": "^5.5.1",
    "@middy/http-error-handler": "^5.5.1",
    "@middy/http-event-normalizer": "^5.0.0",
    "@middy/http-json-body-parser": "^5.0.0",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "aws-lambda": "^1.0.7",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "joi": "^17.12.2",
    "jsonwebtoken": "^9.0.2",
    "nanoid": "^3.3.7",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.133",
    "@types/bcryptjs": "^2.4.6",
    "@types/dotenv": "^6.1.1",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.17",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "aws-sdk-client-mock": "^3.0.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "serverless": "^3.38.0",
    "serverless-esbuild": "^1.50.0",
    "serverless-iam-roles-per-function": "^3.2.0",
    "serverless-offline": "^13.3.3",
    "serverless-plugin-canary-deployments": "^0.7.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "serverless",
    "aws",
    "lambda",
    "dynamodb",
    "api-gateway",
    "enterprise",
    "typescript",
    "clean-architecture"
  ],
  "author": "Enterprise Development Team",
  "license": "MIT"
}
